{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "google",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc": "off",
        "react/prop-types": "off",
        "operator-linebreak": "off",
        "max-len": ["error", 100],
        "indent": ["error", 4]
    }
}
